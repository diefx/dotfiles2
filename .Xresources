!Remove scrollbar
URxvt.scrollBar:      false
URxvt*font: xft:Source Code Pro Medium:size=14:antialias=true:hinting=true,xft:Liberation Mono:size=14:antialias=true:hinting=true
! terminal padding
URxvt.internalBorder: 25

Xft.autohint:   0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    true
Xft.antialias:  true
Xft.rgba: rgb

!Color Scheme to mach with arch theme
! colors
URxvt.cursorColor:  #ebdbb2
! hard contrast: *background: #1d2021
URxvt.background:   #282828
! soft contrast: *background: #32302f
URxvt.foreground:   #ebdbb2

